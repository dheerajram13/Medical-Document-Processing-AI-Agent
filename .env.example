# Azure Document Intelligence
AZURE_DOC_INTELLIGENCE_ENDPOINT=https://your-resource-name.cognitiveservices.azure.com/
AZURE_DOC_INTELLIGENCE_KEY=your-api-key-here

# Google Vertex AI
GOOGLE_PROJECT_ID=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=gemini-2.0-flash-exp

# Google AI Studio (Gemini API)
GEMINI_API_KEY=your-gemini-api-key

# AWS Bedrock (fallback)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# Supabase
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Redis (Bull Queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# AWS S3 (backup storage)
AWS_S3_BUCKET=medical-documents-backup
AWS_S3_REGION=us-east-1

# AI extraction tuning (optional)
AI_PROVIDER=gemini
AI_MAX_OUTPUT_TOKENS=2000
AI_MAX_PROMPT_CHARS=250000
AI_REQUEST_TIMEOUT_MS=0

# OCR tuning (optional)
OCR_POLL_INTERVAL_MS=1000

# Backend CORS allowlist (comma-separated)
# Example: https://your-app.vercel.app,http://localhost:3002
CORS_ORIGINS=http://localhost:3002,http://localhost:3000
